<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <!-- No namespace here as we will share this description. 
       Access with slash at the beginning -->
  <!--<param name="robot_description"
         command="$(find xacro)/xacro.py $(find turtlebot_description)/urdf/turtlebot_gazebo.urdf.xacro" /> -->
  <arg name="station_name" default="" />
  <arg name="location_of_init_bash" />
  
    <!-- STATION 1 
    <machine name="station1" address="station1" user="serl" env-loader="$(arg location_of_init_bash)" />
   -->
    <!-- BEGIN ROBOT 1 
    <group ns="robot1">
        <param name="tf_prefix" value="robot1_tf" />
        
        <include file="$(find multirobot)/station1.machine">
          <arg name="location_of_init_bash" value="$(arg location_of_init_bash)" />
        </include>
        
    </group>
    <include file="$(find multirobot)/launch/one_robot.launch" >
        <arg name="station_name" value="$(arg station_name)" />
        <arg name="init_pose_x" value="-65.388400" />
        <arg name="init_pose_y" value="13.686600"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot1" />
    </include>
    -->
    <!-- BEGIN ROBOT 2 
    <group ns="robot2">
        <param name="tf_prefix" value="robot2_tf" />
               
        <include file="$(find multirobot)/station1.machine">
          <arg name="location_of_init_bash" value="$(arg location_of_init_bash)" />
        </include>
        
    </group>
    <include file="$(find multirobot)/launch/one_robot.launch" >
        <arg name="station_name" value="$(arg station_name)"/>
        <arg name="init_pose_x" value="-11.750800" />
        <arg name="init_pose_y" value="2.632480"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot2" />
    </include>
   -->

    <!-- STATION 4   
    <machine name="station4" address="station4" user="serl" env-loader="$(arg location_of_init_bash)" /> -->  
    <!-- BEGIN ROBOT 3 -->
    <group ns="robot3">
        <param name="tf_prefix" value="robot3_tf" />

        <!--        
        <include file="$(find multirobot)/station4.machine">
          <arg name="location_of_init_bash" value="$(arg location_of_init_bash)" />
        </include>
        -->

    </group>
    <include file="$(find multirobot)/launch/one_robot.launch" >
      <arg name="init_pose_x" value="-82.605500" />
      <arg name="init_pose_y" value="161.682000"/>
      <arg name="init_pose_a" value="3.14"/>
      <arg name="robot_name"  value="robot3" />
    </include>

   <group ns="robot4">
        <param name="tf_prefix" value="robot4_tf" />
   </group>
    <include file="$(find multirobot)/launch/one_robot.launch" >
      <arg name="init_pose_x" value="-11.804200" />
      <arg name="init_pose_y" value="115.885000"/>
      <arg name="init_pose_a" value="3.14"/>
      <arg name="robot_name"  value="robot4" />
    </include>
</launch>
